{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "^2.13",
        "bezhansalleh/filament-shield": "^1.1.12",
        "filament/filament": "^2.16.66",
        "guzzlehttp/guzzle": "^7.5.0",
        "joelbutcher/socialstream": "3.9.1",
        "laravel/framework": "^9.48",
        "laravel/jetstream": "^2.16",
        "laravel/pint": "^1.13.2",
        "laravel/sanctum": "^2.15.1",
        "laravel/socialite": "^5.6.1",
        "laravel/telescope": "^4.16.4",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.11",
        "owenvoke/blade-fontawesome": "^2.1",
        "ralphjsmit/laravel-filament-seo": "^0.7.0",
        "socialiteproviders/discord": "^4.1.1",
        "spatie/data-transfer-object": "^3.9.1",
        "symfony/http-client": "^6.2.5",
        "symfony/mailgun-mailer": "^6.2.5",
        "tailflow/castable-dto": "^2.1",
        "webbingbrasil/filament-advancedfilter": "^1.1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21.0",
        "laravel/sail": "^1.18.1",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4",
        "nunomaduro/larastan": "^2.4",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9.14",
        "phpunit/phpunit": "^9.5.28",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "./vendor/bin/pint",
        "prettier": "npm run prettier",
        "style": [
            "@pint",
            "@prettier"
        ],
        "phpstan": "./vendor/bin/phpstan analyse",
        "phpstan-baseline": "./vendor/bin/phpstan analyse --generate-baseline",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:models -W -R",
            "@style"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": false
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
